requests.get("https://graph.microsoft.com/v1.0/me/drive/root/search(q='myupload.jpg')?select=name,id,webUrl", headers={"Authorization":""}).text



requests.put("https://graph.microsoft.com/v1.0/me/drive/root:/yourfilename.txt:/content", headers={"Authorization":""}).text



filepath = 'yourfilename.txt'
with open(filepath) as file_hander:
    mydata = file_hander.read()
    response = requests.put("https://graph.microsoft.com/v1.0/me/drive/root:/yourfilename.txt:/content", headers={"Authorization":""}, data=mydata)
    print(response.status_code)
